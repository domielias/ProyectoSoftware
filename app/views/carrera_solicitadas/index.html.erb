<p id="notice"><%= notice %></p>

<h1>Carrera Solicitadas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carrera_solicitadas.each do |carrera_solicitada| %>
      <tr>
        <td><%= carrera_solicitada.nombre %></td>
        <td><%= link_to 'Show', carrera_solicitada %></td>
        <td><%= link_to 'Edit', edit_carrera_solicitada_path(carrera_solicitada) %></td>
        <td><%= link_to 'Destroy', carrera_solicitada, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Carrera Solicitada', new_carrera_solicitada_path %>
