<%# <!DOCTYPE html> %>
  <html>
    <head>
      <title>Proyecto Software</title>
      <%= csrf_meta_tags %>
      <%= csp_meta_tag %>
      <meta name="viewport" content="width=device-width, initial-scale=1">

      <%#= stylesheet_link_tag 'application.css', media: 'all', 'data-turbolinks-track': 'reload' %>
      <%#= javascript_pack_tag 'application.js', 'data-turbolinks-track': 'reload' %>
      <!-- Para los dropdowns de select2-->
      <%= stylesheet_link_tag "application.css", media: "all", "data-turbolinks-track" => true %>
      <%= javascript_include_tag "application.js", "data-turbolinks-track" => true %>
      <!-- Para los dropdowns de Scrollbars-->
      <%= stylesheet_link_tag 'OverlayScrollbars.css', media: 'all', 'data-turbolinks-track'=> true %>
      <%= javascript_include_tag 'jquery.overlayScrollbars.js', "data-turbolinks-track" => true %>

    </head>


    <body class="sidebar-mini layout-fixed sidebar-collapse">
      <div class="wrapper">
        <% if current_user %>
          <%= render 'layouts/sidebar' %>
        <% end %>
        <%# render 'layouts/messages' %>
        <%= yield %>
        <div>
          <%# render 'layouts/footer' %>
        </div>
      </div>
    </body>
  </html>
