<p id="notice"><%= notice %></p>

<h1>Programa Internacionals</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programa_internacionals.each do |programa_internacional| %>
      <tr>
        <td><%= programa_internacional.nombre %></td>
        <td><%= link_to 'Show', programa_internacional %></td>
        <td><%= link_to 'Edit', edit_programa_internacional_path(programa_internacional) %></td>
        <td><%= link_to 'Destroy', programa_internacional, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Programa Internacional', new_programa_internacional_path %>
