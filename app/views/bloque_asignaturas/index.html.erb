<p id="notice"><%= notice %></p>

<h1>Bloque Asignaturas</h1>

<table>
  <thead>
    <tr>
      <th>Bloque</th>
      <th>Asignatura</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bloque_asignaturas.each do |bloque_asignatura| %>
      <tr>
        <td><%= bloque_asignatura.bloque_id %></td>
        <td><%= bloque_asignatura.asignatura_id %></td>
        <td><%= link_to 'Show', bloque_asignatura %></td>
        <td><%= link_to 'Edit', edit_bloque_asignatura_path(bloque_asignatura) %></td>
        <td><%= link_to 'Destroy', bloque_asignatura, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bloque Asignatura', new_bloque_asignatura_path %>
